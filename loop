using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace uppgiftlopp2
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Ange ett värde mellan 1 och 100");
            int nummer = int.Parse(Console.ReadLine());
            if (nummer > 100)
            {
                Console.WriteLine("Värdet du angav är större än 100, försök igen.");
            }
            if (nummer < 1)
            {
                Console.WriteLine("Värdet du angav är mindre än 1, försök igen");
            }
            else do
            {
                Console.WriteLine("{0}", nummer);
                nummer++;
            } while (nummer < 101);
        }
    }
}
